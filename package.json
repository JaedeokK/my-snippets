{
  "name": "real-my-snippets",
  "displayName": "My Snippets",
  "description": "These are actually my snippets, not just my snippets by name.",
  "icon": "static/img/cat.png",
  "version": "1.1.0",
  "publisher": "JaedeokKim",
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdcoder27/my-snippets"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "keywords": [
    "snippets",
    "my snippets"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./snippets/react-ts.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/react-ts.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/next-ts.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/next-ts.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/util.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/util.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/util.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/util.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.2"
  }
}
